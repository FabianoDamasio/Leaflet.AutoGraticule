{
  "name": "leaflet-auto-graticule",
  "version": "1.1.1",
  "description": "Leaflet graticule with automatic adaption to zoom level.",
  "main": "./dist/L.AutoGraticule.js",
  "module": "./dist/L.AutoGraticule.mjs",
  "types": "./dist/L.AutoGraticule.d.ts",
  "exports": {
    ".": {
      "import": "./dist/L.AutoGraticule.mjs",
      "require": "./dist/L.AutoGraticule.js"
    },
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FacilMap/Leaflet.AutoGraticule.git"
  },
  "license": "BSD-2-Clause",
  "files": [
    "example.html",
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build",
    "clean": "rimraf dist",
    "dev-server": "vite",
    "check-types": "tsc --noEmit",
    "test": "vitest run"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.3",
    "jsdom": "^21.1.1",
    "leaflet": "^1.9.3",
    "rimraf": "^4.4.1",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-css-injected-by-js": "^3.1.0",
    "vite-plugin-dts": "^2.2.0",
    "vitest": "^0.29.8"
  },
  "peerDependencies": {
    "leaflet": "x"
  },
  "packageManager": "yarn@3.2.0"
}
